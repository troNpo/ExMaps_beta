{
  "name": "ExMaps",
  "short_name": "ExMaps",
  "id": "/",
  "start_url": "./index.html",
  "scope": "./",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "orientation": "portrait",
  "description": "b√∫squeda de pois.",
  "background_color": "#000000",
  "theme_color": "#000000",
  "lang": "es-ES",
  "dir": "ltr",
  "categories": ["navigation", "utilities", "travel"],
  "iarc_rating_id": "",
  "prefer_related_applications": false,
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "icons": [
    {
      "src": "icons/logo192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/logo512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "icons/screenshot1.png",
      "sizes": "720x1541",
      "type": "image/png"
    },
    {
      "src": "icons/screenshot2.png",
      "sizes": "720x1541",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "/abrir",
      "accept": {
        "application/pdf": [".pdf"],
        "application/json": [".json"]
      }
    }
  ],
  "handle_links": {
    "enabled": true,
    "include": ["https://exmaps.es/*"]
  },
  "protocol_handlers": [
    {
      "protocol": "exmaps",
      "url": "https://exmaps.es/ruta?dato=%s"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 400
  }
}
